/**index.wxss**/
.u-wrp-bnr {
  width: 100%;
  height: 400rpx;
  display: block;
  position: relative;
  background: #f0f0f0;
}
.u-img-slide {
  width: 100%;
  height: inherit;
}
.title {width:100%;height:106rpx;line-height: 106rpx;}
.title .imgs {width:110rpx;height:100%;float:left;margin-left: 40rpx;line-height: 106rpx;}
.title image {width:110rpx;height:60rpx;vertical-align: middle;}
.titleText {margin-left: 178rpx;width:70%;line-height: 106rpx;font-size: 27rpx;color: #000;
white-space: nowrap;
overflow: hidden;
text-overflow: ellipsis;
}
.sort {width:100%;height:170rpx;position: relative;padding-top: 10rpx;}
.sotrDetail {width:25%;float:left;height:100%;position: relative;text-align: center;}
.sotrDetail image {width:115rpx;height:115rpx;}
.sotrDetail view {font-size: 18rpx;}
.news {width:100%;position: relative;padding: 12rpx 0;}
.newsLeft {width:333rpx;height:343rpx;float:left;position: relative;margin-left: 20rpx;}
.newsRight {width:333rpx;height:343rpx;float:left;position: relative;margin-left: 48rpx;}
.newsLeft image {width:333rpx;height:343rpx;}
.newsText {width:126rpx;height:35rpx;text-align: center;line-height: 35rpx;background: #dac9a6;font-size: 20rpx;color: #fff;
  border-radius: 20rpx;position: absolute;top:22rpx;right:10rpx;
}
.newsNum {border-top-right-radius: 20rpx;border-bottom-right-radius: 20rpx;background: #dac9a6;
  width:90rpx;height:45rpx;text-align: center;font-size: 26rpx;color: #fff;line-height: 45rpx;
  position: absolute;bottom:33rpx;left:0;
}
.newsNum text {font-size: 16rpx;margin-right: 10rpx;}
.projectTop {width:100%;height:150rpx;text-align: center;}
.projectTop01 {font-size: 32rpx;margin-top: 45rpx;}
.projectTop02 {font-size: 20rpx;margin-top: 22rpx;}
.detail {width:96%;position: relative;margin: auto;}
.detailSort {width:100%;display: block;background: #dac9a6;position: relative;border-radius: 10rpx;height:257rpx;
  padding: 20rpx 0;margin-bottom: 16rpx;
} 
.detailLeft {width:260rpx;position: relative;height:257rpx;margin-left: 40rpx;float:left;}
.detailLeft01 {font-size: 32rpx;color: #000;
text-overflow: -o-ellipsis-lastline;

overflow: hidden;

text-overflow: ellipsis;

display: -webkit-box;

-webkit-line-clamp: 2;

-webkit-box-orient: vertical;
}
.detailLeft02 {font-size: 14rpx;}
.detailLeft03 {font-size: 20rpx;}
.detailLeft03 text {font-size: 40rpx;}
.detailLeft04 {width:103rpx;height:26rpx;text-align: center;border:1px solid #7f7664;font-size: 14rpx;line-height: 26rpx;}
.detailLeft05 {position: absolute;bottom:0;left:0;}
.detailRight {margin-left: 360rpx;}
.detailRight image {width:322rpx;height:257rpx;}
.bottom01 {width:54rpx;text-align: center;}
.more {font-size: 22rpx;color: #000;margin: 50rpx 0;}